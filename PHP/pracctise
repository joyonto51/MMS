
<input id='number1' type='number'> <br>
<input id='number2' type='number'> <br>
<input id='number3' type='number'> <br>
<button onclick="sum()">Submit</button> 


<script>

function sum(){

a=document.getElementById('number1').value;
b=document.getElementById('number2').value;
c=document.getElementById('number3').value;

number1=parseInt(a)
number2=parseInt(b)
number3=parseInt(c)
console.log(number1+number2+number3);
}



</script> 